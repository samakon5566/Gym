    <style>

        .coach-nav > label {
            color: white;
            font-size: 30px;
        }

        #icon-19 > i {
            font-size: 100px;
            position: fixed;
            bottom: 20px;
            right: 20px;
            border-radius: 50%;
            z-index: 99;
            color: orange;
            /*border: 2px solid #81C81E;*/
            background-color: black;
        }

        .chat-19 {
            width: 350px;
            height: 360px;
            bottom: 0px;
            position: fixed;
            right: 30px;
            z-index: 99;
            border: 2px solid black;
        }

        .active-19 {
            bottom: -297px;
        }

        .controler-19 {
            text-align: right;
            width: 100%;
            height: 30px;
            background-color: #BEBEBE;
            /*            border: 2px solid red;*/
        }

        #chat-19 .chat_message {
            padding: 10px;
            width: 100%;
            height: 90%;
            overflow-y: scroll;
            display: flex;
            flex-direction: column;
            background-color: #F2F2F2;
            /*padding: 0 20px;*/
        }

        #chat-19 .chat_input {
            width: 90%;
            height: 10%;
            display: flex;
            background-color: #fff;
            padding: 0 20px;
            position: relative;
        }

        #controler-icon-19 {
            margin-right: 15px;
            font-size: 30px;
            color: #2894FF;
        }

        #controler-icon1-19 {
            margin-right: 10px;
            font-size: 30px;
            color: red;
        }

        .footer-section > .container > .row > .col-lg-4 > .fs-about > p:hover {
            color: white;
        }

        .footer-section .fs-widget ul > li > a:hover {
            color: white;
        }

        .div-Coach-personal {
            width: 100%;
            display: flex;
            height: 100px;
            margin-top: 20px;
            justify-content: space-evenly;
        }

        .div-Coach-personal-pic {
            width: 30%;
            height: 100%;
            border-radius: 50%;
            overflow: hidden;
        }

            .div-Coach-personal-pic img {
                object-fit: cover;
                object-position: center;
            }

        .div-Coach-personal-name {
            color: white;
            font-size: 36px;
            line-height: 100px;
        }

        .ul-Coach-nav {
            text-align: left;
            margin: 30px 40px 0 30px;
            font-size: 24px;
            color: white;
        }

            .ul-Coach-nav li {
                border-bottom: solid 1px #ffffff3d;
                padding-bottom: 24px;
                margin-top: 18px;
                position: relative;
                color: white;
            }

                .ul-Coach-nav li div {
                    color: white
                }

                .ul-Coach-nav li:after {
                    content: '';
                    position: absolute;
                    bottom: 0;
                    left: 0;
                    background-color: #fff;
                    width: 0%;
                    height: 1px;
                    -webkit-transition: all 0.7s ease;
                    -moz-transition: all 0.7s ease;
                    -o-transition: all 0.7s ease;
                    transition: all 0.7s ease;
                }

                .ul-Coach-nav li:hover:after {
                    width: 100%;
                    -webkit-transition: all 0.7s ease;
                    -moz-transition: all 0.7s ease;
                    -o-transition: all 0.7s ease;
                    transition: all 0.7s ease;
                }
    </style>
